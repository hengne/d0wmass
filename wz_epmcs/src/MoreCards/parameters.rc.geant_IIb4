radLengthcenter_EC: 1
radLengthstep_EC: 1



#
# step sizes for A, B and C
#
A_step:   0.0001
B_step:   0.001
C_step:   0.0005

# parameters used for J/Psi->mumu analysis
CutJPsiMuPT:           2.5

# parameters used for Z->mumu analysis
CutZMuPT:              20.

#
# output file
#
output_file_jpsimumu: result_jpsimumu.root
output_file_zmumu: result_zmumu.root
output_file_znunu: result_znunu.root

#
# ZNuNuAnalysis
#
ZNuNu_NuLoose_nMin:   0
ZNuNu_NuLoose_EtaMax: 2.5
ZNuNu_NuLoose_PtMin:  0.0
ZNuNu_NuTight_nMin:   2
ZNuNu_NuTight_EtaMax: 1.3
ZNuNu_NuTight_PtMin:  0.0
ZNuNu_UtCutMin:       0.0
ZNuNu_UtCutMax:       1000.0
ZNuNu_SETCutMin:      0.0
ZNuNu_SETCutMax:      10000.0
ZNuNu_dumpRecoilInfo: false
ZNuNu_dumpRecoilFile: znunu_recoil_dump.txt

# Parameters for TB Asym Correction
TBCorrection: false
TBCorrection_Option: 2

# only for TBCorrection_Option: 1
TBApplyLast: false
TBPhi_response_Hard_Only: false
TBPhi_response_Soft_Only: true
TBalpha: 0.222
TBbeta: -0.509
TBStudy: false
TBmodel_patrice: false
tb_alpha_center: 0.222
tb_beta_center: -0.509
tbalpha_step: 0.001
tbbeta_step: 0.002
Bins_TBStudy: 0

# only for TBCorrection_Option: 2
TBLibrary_file: tblib_fix3_3d.root  
               # Don't forget to setenv/export $TBLibraryRootPath,
               # Otherwise it will choose the runtime directory.

# Configuration parameters for Hadronic Recoil Study
# ZHadronicBifurcateStudy: Turns on and off the study mode. (default = FALSE)
ZHadronicRecoilBifurcateStudy: false
# Number of bins in the template, the order is:
#     RelScaleA
#     RelScaleB
#     tau_HAD
#     RelSampA
#     RelSampB
#     alpha_MB
#     RelPhiA
#     RelPhiB
ZRecoilBifurcateBins:          0 0 0 0 0 0 0 0
# Step for each of the variables above 
ZRecoilBifurcateStep:          0.001 0.005 0.05 0.003 0.0025 0.003 0.001 0.01
# Name of the output file - it does not write in result_zee.root!
ZRecoilBifurcateFile:          recoil_tree.root
ZRecoilDebugLevel:             0

TransZHadronicRecoilBifurcateStudy: false 
# Number of bins in the template, the order is:
#     RespTrans
#     ResnTrans
#     TauTrans
#     alpha_mb_trans
TransZRecoilBifurcateBins:          0 0 0 0
# Step for each of the variables above 
TransZRecoilBifurcateStep:          0.001 0.01 0.01 0.01

# Apply Z pT x dPhi reweight to fit the recoil
applyDPhiReweight: false
DPhiReweightFile: ../geant_RunIIb/dphi_reweight.C
applyDPhiZPtReweight: false
DPhiZPtReweightFile: ../geant_RunIIb/dphi_vs_zpt_reweight.C
applyReweightPerLumi: false
DPhiZPtReweightFile0: ../geant_RunIIb/dphi_vs_zpt_reweight_0.C
DPhiZPtReweightFile2: ../geant_RunIIb/dphi_vs_zpt_reweight_2.C
DPhiZPtReweightFile4: ../geant_RunIIb/dphi_vs_zpt_reweight_4.C
DPhiZPtReweightFile6: ../geant_RunIIb/dphi_vs_zpt_reweight_6.C

# fill ZBNum Tree
FillZBRunNumTree: false

